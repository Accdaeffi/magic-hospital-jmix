<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.11.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="magic-hospital">
        <addNotNullConstraint columnDataType="VARCHAR(255)" columnName="FIRST_NAME" tableName="USER_" validate="true"/>
    </changeSet>
    <changeSet id="2" author="magic-hospital">
        <addNotNullConstraint columnDataType="VARCHAR(255)" columnName="name" tableName="PATIENT" validate="true"/>
    </changeSet>
    <changeSet id="3" author="magic-hospital">
        <addNotNullConstraint columnDataType="UUID" columnName="patient_id" tableName="DISEASE_CASE" validate="true"/>
    </changeSet>
</databaseChangeLog>